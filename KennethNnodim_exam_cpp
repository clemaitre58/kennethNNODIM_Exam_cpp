

#include <iostream>
using namespace std;

class Coffee_Machine {
    int water_level, coffee_level, max_water_level,max_coffee_level;
	max_coffee_level = 10;
	max_water_level = 10;
  public:
    void set_values (int,int);
    int Done () {return width*height;}
};

void Coffee_Machine::set_values (int x, int y) {
  water_level = x;
  coffee_level = y;
}

int main () {
  Coffee_Machine Mixed , Mixed2;
  Mixed.set_values (3,4);
  Mixed2.set_values (5,6);
  cout << " Mixed Done: " << Mixed.Done() << endl;
  cout << "Mixed2 Done: " << Mixed2.Done() << endl;
  return 0;
}